package com.raviraj.app;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.context.ApplicationContext;

@SpringBootApplication
public class SpringBootDiApplication {

    private final Computer computer;

    SpringBootDiApplication(Computer computer) {
        this.computer = computer;
    }

	public static void main(String[] args) {
		ApplicationContext context = SpringApplication.run(SpringBootDiApplication.class, args);
		
		Alien a = context.getBean("alien", Alien.class);
		a.print();
		Computer lp = a.getCom();
		lp.compile();
		
	}

}
