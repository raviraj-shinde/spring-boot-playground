package com.raviraj.app;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.beans.factory.annotation.Value;
import org.springframework.stereotype.Component;

@Component
public class Alien {
	// Properties

	private Computer com;

//	@Value("1")  //field Injection
	private int age;

	// Constructor

	public Alien( @Autowired Computer com, @Value("23") int age) {  // Constructor Injection (Recommended)
		this.com = com;
		this.age = age;
	}

	// Getters and Setters
	public Computer getCom() {
		return com;
	}

//	@Autowired                        //Settr Injection
//	@Qualifier("laptop")
//	public void setCom(Computer com) {
//		this.com = com;
//	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	// Function
	public void print() {
		System.out.println("Alien Bean print() Called");
	}

}
