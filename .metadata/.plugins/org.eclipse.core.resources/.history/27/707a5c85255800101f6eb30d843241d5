package com.raviraj.app;

public class Seller {
	Desktop hp;
	private int price;

	public Desktop getHp() {
		System.out.println(" HP Desktop Given to Alien");
		return hp;
	}

	public void setHp(Desktop hp) {
		this.hp = hp;
	}

	public int getPrice() {
		return price;
	}

	public void setPrice(int price, int count) {  //setters are called at the Spring Bean Creation
		this.price = price * count;
	}
	
	
	
	
}
