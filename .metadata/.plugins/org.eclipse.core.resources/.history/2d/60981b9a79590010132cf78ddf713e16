package com.raviraj.app;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.beans.factory.annotation.Qualifier;
import org.springframework.beans.factory.annotation.Value;
import org.springframework.stereotype.Component;

@Component
public class Alien {

// Properties
	private final Computer com1;
	private final Computer com2;
	// @Value("1") //field Injection
	private int age;

// Constructor
	// no requires @Autowire from Spring v4.3+
	public Alien(Computer com1, @Qualifier("laptop") Computer com2, @Value("23") int age) {  // Constructor Injection (Recommended)
		this.com1 = com1;
		this.com2 = com2;
		this.age = age;
	}

// Getters and Setters
	public Computer getCom() {
		return com;
	}

//	@Autowired                        //Setter Injection
//	@Qualifier("laptop")
//	public void setCom(Computer com) {
//		this.com = com;
//	}

	public void setAge(int age) {
		this.age = age;
	}

	// Function
	public void print() {
		System.out.println("Alien Bean print() Called");
	}

}
